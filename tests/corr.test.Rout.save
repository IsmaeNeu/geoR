> tc <- function(x, phi, kappa, cov.model)
+ {
+   res <- rep(0.0, length(x))
+   .C("veccorrval",
+      as.double(phi),
+      as.double(kappa),
+      as.double(x),
+      as.integer(length(x)),
+      cornr = as.integer(cor.number(cov.model)),
+      out = as.double(res))$out
+ }
> 
> x <- 1:100
> for(cm in c("exponential", "matern", "gaussian",
