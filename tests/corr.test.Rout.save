+   {
+     kp <- 1.9
+     x1 <- tc(x, 30, kp, cm)
+     x2 <- cov.spatial(x, cm, cov.pars=c(1, 30), kappa = kp)
+     if(all(x1-x2) < 1e-8)
+       cat(paste(cm, "OK\n"))
+     else{
+       cat(cm)
+       cat("\n")
+     }
+     print(x1)
+     print(x2)
+   }
